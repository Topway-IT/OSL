        "en": "Postal address",
        "de": "Postalische Adresse"
    },
    "description": "",
    "eval_template": {
        "type": "wikitext",
        "mode": "render",
        "value": "{{{street_name|}}} {{{house_no|}}}<br>{{#if: {{{address_supplement|}}}|{{{address_supplement|}}}<br>|}}{{{postal_code|}}} {{#if: {{{city|}}} |[[{{{city|}}}]]|{{{city_name|}}} }}, {{#if: {{{region|}}}|[[{{{region|}}}]],&nbsp;|}}{{#if: {{{state|}}}|[[{{{state|}}}]],&nbsp;|}}{{#if: {{{country|}}} |[[{{{country|}}}]]|{{{country_name|}}} }}{{#if: {{{post_office_box_no|}}}|<br>{{#switch:{{USERLANGUAGECODE}}|#default=PO Box No|de=Postfach-Nr.}} {{{post_office_box_no|}}}|}}"
