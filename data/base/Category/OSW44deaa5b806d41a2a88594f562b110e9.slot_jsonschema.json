                    "de": "Anzeigenamen"
                },
                "properties": {
                    "text": {
                        "watch": {
                            "first_name": "root.first_name",
                            "middle_name": "root.middle_name",
                            "surname": "root.surname"
                        },
                        "readonly": true,
                        "dynamic_template": "{{first_name}}{{#each middle_name}} {{.}}{{/each}}{{#if surname}} {{{surname}}}{{/if}}"
                    },
                    "lang": {
                        "type": "string",
                        "default": "en"
                    }
                }
            },
            "options": {
                "hidden": true
            }
        },
        "organization": {
